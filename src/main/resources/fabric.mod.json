{
  "schemaVersion": 1,
  "id": "yoinker",
  "version": "${version}",

  "name": "Figura Avatar Yoinker",
  "description": "Alows you to Yoink Figura Models",
  "authors": [
    "Virusnest"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "dev.virusnest.yoinker.Yoinker"
    ]
  },
  "mixins": [
    "yoinker.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric-api": "*",
    "minecraft": "~1.19",
    "figura": ">=${figura}",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
